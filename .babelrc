{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": { "version": 3, "proposals": true },
        "targets": "> 0.25%, not dead, IE >= 9",
        "useBuiltIns": "usage",
        "modules": "umd"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-external-helpers",
    [
      "@babel/plugin-transform-runtime",
      {
        "helper": true,
        "regenerator": true
      }
    ]
  ]
}
